{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "The derivations for a provenance chain for an address",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "type": {
        "type":"string",
        "enum": ["Source", "userInput"]
      },
      "input": {"type":"string"},
      "urls": {
        "type":"array",
        "items": {
          "type": "string",
          "format":"uri"
        }
      },
      "inputted_at": {
        "type":"string",
        "format":"date-time"
      },
      "downloaded_at": {
        "type":"string",
        "format":"date-time"
      },
      "processing_script":{
        "type":"string",
        "format":"uri"
      }
    },
    "required": ["type", "processing_script"],
    "dependencies": {
      "input": ["inputted_at"],
      "urls": ["downloaded_at"]
    }
  }
}
